<SimOptions>
  <GlobalController>
     <Port>8888</Port>
     <StartLocalControllers>true</StartLocalControllers>
     <ConnectedNetwork>true</ConnectedNetwork>
  </GlobalController>
  <LocalController>
     <Name>localhost</Name>
     <Port>10000</Port>
     <LowPort>11001</LowPort>
     <HighPort>12000</HighPort>
     <MaxRouters>20</MaxRouters>
  </LocalController>
  <EventEngine>
     <Name>usr.engine.ProbabilisticEventEngine</Name>
     <EndTime>36</EndTime>
     <Parameters>scripts/probdistslow_noPA.xml</Parameters>
  </EventEngine>
  <EventEngine>
     <Name>usr.engine.BackgroundTrafficEngine</Name>
     <EndTime>36</EndTime>
     <Parameters>scripts/backtraffdists.xml</Parameters>
  </EventEngine>
  <RouterOptions>
      scripts/routeroptions.xml
  </RouterOptions>
      <Output>
        <File>out.net</File>
        <When>End</When>
        <Type>Network</Type>
    </Output>
    <Output>
        <File>summary.out</File>
        <When>Interval</When>
        <Time>10000</Time>
        <Type>Summary</Type>
    </Output>
        <Output>
        <File>traffic.out</File>
        <When>Interval</When>
        <Time>10000</Time>
        <Type>Traffic</Type>
        <Parameter>Aggregate</Parameter>
    </Output>
    <Output>
        <File>traffic.raw</File>
        <When>Interval</When>
        <Time>10000</Time>
        <Type>Traffic</Type>
        <Parameter>Raw</Parameter>
    </Output>

</SimOptions>
