<SimOptions>
  <GlobalController>
     <Port>8888</Port>
     <ConnectedNetwork>false</ConnectedNetwork>
     <Simulation>true</Simulation>
  </GlobalController>
  <EventEngine>
     <Name>Probabilistic</Name>
     <EndTime>3600</EndTime>
     <Parameters>monitoring_scripts/sim_probdists_noPA.xml</Parameters>
  </EventEngine>
  <RouterOptions>
      monitoring_scripts/sim_routeroptions.xml
  </RouterOptions>
      <Output>
        <File>out.net</File>
        <When>End</When>
        <Type>Network</Type>
    </Output>
    <Output>
        <File>summary.out</File>
        <When>Interval</When>
        <Time>10000</Time>
        <Type>Summary</Type>
    </Output>
    <Output>
        <File>traffic.agg</File>
        <When>Interval</When>
        <Time>10000</Time>
        <Type>Traffic</Type>
        <Parameter>Aggregate</Parameter>
    </Output>

</SimOptions>
